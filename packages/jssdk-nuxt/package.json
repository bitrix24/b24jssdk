{
  "name": "@bitrix24/b24jssdk-nuxt",
  "version": "1.0.1",
  "packageManager": "pnpm@10.20.0",
  "author": "Bitrix",
  "license": "MIT",
  "description": "Bitrix24 REST API JS SDK for Nuxt",
  "keywords": [
    "bitrix24",
    "restapi",
    "nuxt"
  ],
  "homepage": "https://bitrix24.github.io/b24jssdk/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitrix24/b24jssdk.git",
    "directory": "packages/jssdk-nuxt"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./runtime/*": "./dist/runtime/*"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/module.d.mts"
      ],
      "runtime/*": [
        "./dist/runtime/*"
      ]
    }
  },
  "files": [
    "README.md",
    "LICENSE",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare",
    "prepack": "pnpm build",
    "build": "nuxt-module-build build",
    "typecheck": "nuxt typecheck",
    "test": "vitest run"
  },
  "dependencies": {
    "@nuxt/kit": "^4.2.1",
    "@bitrix24/b24jssdk": "workspace:*",
    "axios": "^1.13.2",
    "qs-esm": "^7.0.2",
    "luxon": "^3.7.2"
  },
  "devDependencies": {
    "@nuxt/test-utils": "^3.21.0",
    "@types/luxon": "^3.7.1",
    "vitest": "^4.0.13",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.2.1",
    "@types/node": "^25.0.0",
    "nuxt": "^4.2.1",
    "typescript": "^5.8.3",
    "vue-tsc": "^3.1.5"
  }
}
