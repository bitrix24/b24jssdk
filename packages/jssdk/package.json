{
  "name": "@bitrix24/b24jssdk",
  "version": "1.0.1",
  "author": "Bitrix",
  "license": "MIT",
  "description": "Bitrix24 REST API JS SDK",
  "keywords": [
    "bitrix24",
    "js",
    "nodejs",
    "restapi",
    "REST",
    "API"
  ],
  "homepage": "https://bitrix24.github.io/b24jssdk/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitrix24/b24jssdk.git",
    "directory": "packages/jssdk"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/esm/index.d.mts",
      "import": "./dist/esm/index.mjs"
    }
  },
  "main": "./dist/esm/index.mjs",
  "module": "./dist/esm/index.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/esm/index.d.mts"
      ]
    }
  },
  "unpkg": "dist/umd/index.min.js",
  "files": [
    "README.md",
    "README-AI.md",
    "LICENSE",
    "dist",
    "package.json"
  ],
  "engines": {
    "node": "^18.0.0 || ^20.0.0 || >=22.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev:prepare": "pnpm run build",
    "prepack": "pnpm run build",
    "build": "unbuild",
    "typecheck": "tsc --noEmit",
    "test": "tsx ./test/esm/uuid.test.ts && tsx ./test/esm/type.test.ts"
  },
  "dependencies": {
    "axios": "^1.13.2",
    "qs-esm": "^7.0.2",
    "luxon": "^3.7.2"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^25.0.2",
    "ts-node": "^10.9.2",
    "unbuild": "^3.6.1",
    "tsx": "^4.21.0"
  }
}
