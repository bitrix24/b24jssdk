{
  "name": "@bitrix24/b24jssdk-main",
  "version": "0.1.0",
  "author": "Bitrix",
  "license": "MIT",
  "private": true,
  "description": "Bitrix24 REST API JS SDK",
  "keywords": [
    "bitrix24",
    "restapi"
  ],
  "homepage": "https://bitrix24.github.io/b24jssdk/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bitrix24/b24jssdk.git"
  },
  "type": "module",
  "workspaces": [
    "packages/*",
    "playgrounds/*",
    "docs"
  ],
  "scripts": {
    "build": "npm-run-all -s build:*",
    "build:jssdk-unbuild": "npm run --workspace=@bitrix24/b24jssdk build",
    "playground-jssdk": "npm run --workspace=@bitrix24/b24jssdk-playground dev",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "vitepress": "^1.3.4"
  }
}