{
	"name": "@bitrix24/b24jssdk",
	"version": "0.1.2",
	"author": "Bitrix",
	"license": "MIT",
	"description": "Bitrix24 REST API JS SDK",
	"keywords": [
		"bitrix24",
		"js",
		"restapi",
		"REST",
		"API"
	],
	"homepage": "https://bitrix24.github.io/b24jssdk/",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bitrix24/b24jssdk.git",
		"directory": "packages/jssdk"
	},
	"sideEffects": false,
	"type": "module",
	"exports": {
		"./package.json": "./package.json",
		".": {
			"types": {
				"require": "./dist/commonjs/index.d.cts",
				"import": "./dist/esm/index.d.mts",
				"default": "./dist/esm/index.d.ts"
			},
			"default": {
				"require": "./dist/commonjs/index.cjs",
				"import": "./dist/esm/index.mjs",
				"default": "./dist/esm/index.mjs"
			}
		}
	},
	"main": "./dist/commonjs/index.cjs",
	"module": "./dist/esm/index.mjs",
	"types": "./dist/esm/index.d.ts",
	"unpkg": "dist/umd/index.min.js",
	"files": [
		"README.md",
		"LICENSE",
		"dist",
		"package.json"
	],
	"engines": {
		"node": ">=18.0.0"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepack": "npm run build",
		"build": "npm-run-all -s build:*",
		"build:lint-prettier": "eslint . && prettier -c .",
		"build:unbuild": "unbuild",
		"lint": "eslint .",
		"lint:prettier": "eslint . && prettier -c .",
		"lint:fix": "eslint . --fix && prettier -w ."
	},
	"dependencies": {
		"axios": "^1.7.7",
		"qs": "^6.13.0",
		"luxon": "^3.5.0"
	},
	"devDependencies": {
		"@types/luxon": "^3.4.2",
		"@types/node": "^22.5.3",
		"@types/qs": "^6.9.15",
		"eslint-config-unjs": "^0.4.1",
		"prettier": "^3.3.3",
		"npm-run-all": "^4.1.5",
		"typescript": "latest",
		"unbuild": "^3.0.0-rc.11",
		"vitest": "^2.0.5"
	},
	"resolutions": {}
}
